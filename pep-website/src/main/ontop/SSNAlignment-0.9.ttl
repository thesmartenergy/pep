@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix vs: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cc: <http://creativecommons.org/ns#>.
@prefix ssn: <https://www.w3.org/ns/ssn/>.

@prefix pep: <https://w3id.org/pep/>.
@base <https://w3id.org/pep/>.

pep:SSNAlignment a owl:Ontology , voaf:Vocabulary , foaf:Document ;
  dc:title "PEP-SSN Alignments"@en ;
  dc:description "The SSN alignment proposes alignments between the PEP ontology and the SSN ontology."@en ;
  dc:issued "2016-08-26"^^xsd:date ;
  dc:modified "2016-09-22"^^xsd:date ;
  dc:creator <http://www.maxime-lefrancois.info/me#> ;
  cc:license <https://www.apache.org/licenses/LICENSE-2.0> ;
  vann:preferredNamespacePrefix "pep" ;
  vann:preferredNamespaceUri <https://w3id.org/pep/> ;  
  owl:imports pep:, ssn: ;
  owl:versionIRI <SSNAlignment-0.9> ;
  owl:versionInfo "v0.9" .

pep:Process rdfs:subClassOf ssn:Process .

ssn:Sensor rdfs:subClassOf pep:ProcessExecutor .

ssn:Observation rdfs:subClassOf pep:ProcessExecution .

pep:hasInput rdfs:subPropertyOf ssn:hasInput .
ssn:hasInput a owl:ObjectProperty .

pep:hasOutput rdfs:subPropertyOf ssn:hasOutput .
ssn:hasOutput a owl:ObjectProperty .

ssn:implements rdfs:subPropertyOf pep:implements .

ssn:observedBy rdfs:subPropertyOf pep:executedBy .

ssn:sensingMethodUsed rdfs:subPropertyOf pep:methodUsed .
