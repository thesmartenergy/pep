<rdf:RDF xmlns="http://ns.inria.fr/edelweiss/2011/rule#"
xmlns:rdf='http://www.w3.org/1999/02/22-rdf-syntax-ns#'>
  <rule><body><![CDATA[
    # generates the body of the JSON message
    # starting point, called once.
    PREFIX st: <http://ns.inria.fr/sparql-template/>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
    PREFIX ex: <http://example.com/>
    BASE <http://example.com/>
    TEMPLATE st:start {
     "<?xml version='1.0' encoding='UTF-8'?> \n"
     "<messages>\n"
     st:call-template(<https://w3id.org/pep/example/inputGraph/xml/loweringRule#printNumber1>)
     st:call-template(<https://w3id.org/pep/example/inputGraph/xml/loweringRule#printNumber2>)
     "</messages>\n"
    }
    WHERE { }
  ]]></body></rule>
  <rule><body><![CDATA[
    PREFIX st: <http://ns.inria.fr/sparql-template/>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
    PREFIX ex: <http://example.com/>
    BASE <http://example.com/>
    TEMPLATE <https://w3id.org/pep/example/inputGraph/xml/loweringRule#printNumber1> {
    "  <message id='" str(?id) "' min= '"str(?min)"' max='" str(?max) "'/>\n"
    }
    WHERE { 
      ?id ex:min ?min ; ex:max ?max .
      # extract identifier from URI !
  } ORDER BY ?id
    LIMIT 1
  ]]></body></rule>
  <rule><body><![CDATA[
    PREFIX st: <http://ns.inria.fr/sparql-template/>
    PREFIX xsd: <http://www.w3.org/2001/XMLSchema#> 
    PREFIX ex: <http://example.com/>
    BASE <http://example.com/>
    TEMPLATE <https://w3id.org/pep/example/inputGraph/xml/loweringRule#printNumber2> {
    "  <message id='" str(?id) "' min= '"str(?min)"' max='" str(?max) "'/>\n"
    }
    WHERE { 
      ?id ex:min ?min ; ex:max ?max .
      # extract identifier from URI !
  } ORDER BY ?id
    OFFSET 1
  ]]></body></rule></rdf:RDF>