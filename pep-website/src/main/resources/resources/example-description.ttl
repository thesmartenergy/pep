@prefix xsd:  <http://www.w3.org/2001/XMLSchema#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl:  <http://www.w3.org/2002/07/owl#>.
@prefix pep: <https://w3id.org/seas/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix voaf: <http://purl.org/vocommons/voaf#>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix rdfp: <https://w3id.org/rdfp/>.

@prefix pep: <https://w3id.org/pep/>.

@base <https://w3id.org/pep/>.


<example> a foaf:Document ;
  dc:title "Random number generator process execution container"@en ;
  dc:description """This document describes the random number generator process execution container that is hosted on this website."""@en ;
  dc:issued "2016-09-01"^^xsd:date ;
  dc:modified "2016-09-01"^^xsd:date ;
  dc:author <http://www.maxime-lefrancois.info/me#> .

<RandomNumberGenerator> a pep:ProcessExecutor ;
  rdfs:label "Random Number Generator" ;
  rdfs:comment """This is a random number generator, a process executor.""" ;
  pep:implements <RandomNumberGenerationAlgorithm> ;
  pep:processExecutionContainer <RandomNumberGeneration> ;
  rdfs:isDefinedBy <example> .

<RandomNumberGeneration> a pep:ProcessExecutionContainer ;
  rdfs:label "Random Number Generation" ;
  rdfs:comment """This is the container for random number generations, a process execution container.""" ;
  pep:executor <RandomNumberGenerator> ;
  pep:forProcess <RandomNumberGenerationAlgorithm> ;
  rdfs:isDefinedBy <example> .

<RandomNumberGenerationAlgorithm> a pep:Process ;
  rdfs:label "Random Number Generation Algorithm" ;
  rdfs:comment """This is the algorithm used to randomly generate numbers, a process.""" ;
  pep:hasInput <input> ;
  pep:hasOutput <output> ;
  rdfs:isDefinedBy <example> .



<input> a rdfp:GraphDescription ;
  rdfs:label "input graph" ;
  rdfs:comment """This is the description of the input graph.""" ;
  rdfs:comment "The input contains the expressed charging needs for all of the connected electric vehicles in the electric vehicle service equipment pools."@en ;
  rdfp:validationRule <input/validation> ;
  rdfp:presentedBy <input/xml> ;
  rdfp:presentedBy <input/json> ;
  rdfs:isDefinedBy <example> .

<input/xml> a rdfp:Presentation ;
  rdfp:mediaType "application/xml" ; 
  rdfp:liftingRule <input/xml/liftingRule> ;
  rdfp:loweringRule <input/xml/loweringRule> ;
  rdfs:isDefinedBy <example> .

<input/json> a rdfp:Presentation ;
  rdfp:mediaType "application/json" ; 
  rdfp:liftingRule <input/json/liftingRule> ;
  rdfp:loweringRule <input/json/loweringRule> ;
  rdfs:isDefinedBy <example> .



<output> a rdfp:GraphDescription ;
  rdfs:comment "The output contains charging plans for all of the connected electric vehicles in the electric vehicle service equipment pools."@en ;
  rdfp:validationRule <output/validation> ;
  rdfp:presentedBy <output/xml> ;
  rdfp:presentedBy <output/json> ;
  rdfs:isDefinedBy <example> .

<output/xml> a rdfp:Presentation ;
  rdfp:mediaType "application/xml" ; 
  rdfp:liftingRule <output/xml/liftingRule> ;
  rdfp:loweringRule <output/xml/loweringRule> ;
  rdfs:isDefinedBy <example> .

<output/json> a rdfp:Presentation ;
  rdfp:mediaType "application/json" ; 
  rdfp:liftingRule <output/json/liftingRule> ;
  rdfp:loweringRule <output/json/loweringRule> ;
  rdfs:isDefinedBy <example> .
